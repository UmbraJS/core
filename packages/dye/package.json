{
  "name": "@umbrajs/dye",
  "private": false,
  "version": "0.0.102",
  "description": "Vue color picker using umbra",
  "author": "Samuel M. Bednarz<https://github.com/CarelessCourage>",
  "license": "MIT",
  "keywords": [
    "vue",
    "color",
    "picker",
    "umbra"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/umbrajs/dye"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/dye.umd.cjs",
  "module": "./dist/dye.js",
  "types": "./dist/types/index.d.ts",
  "style": "./dist/style.css",
  "exports": {
    ".": {
      "import": "./dist/dye.js",
      "require": "./dist/dye.umd.cjs"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && vue-tsc --emitDeclarationOnly",
    "types": "vue-tsc ",
    "preview": "vite preview"
  },
  "dependencies": {
    "@umbrajs/core": "^0.0.454",
    "@vueuse/components": "^10.1.2",
    "@vueuse/core": "^10.1.2",
    "colord": "^2.9.3",
    "nearest-color": "^0.4.4",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@types/node": "^20.2.1",
    "@vitejs/plugin-vue": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "sass": "^1.56.2",
    "typescript": "^4.9.3",
    "vite": "^4.3.8",
    "vite-plugin-dts": "^2.3.0",
    "vue-tsc": "^1.0.11"
  }
}
