{
  "name": "@myriadjs/core",
  "version": "0.0.942",
  "description": "MyriadJS is a flexible theme managment library that allows you to create semantic color themes that are easy to change retroactivley",
  "author": "Samuel M. Bednarz<https://github.com/CarelessCourage>",
  "repository": {
    "type": "git",
    "url": "https://github.com/MyriadJS/core"
  },
  "bugs": {
    "url": "https://github.com/MyriadJS/core/issues"
  },
  "license": "MIT",
  "keywords": [
    "colors",
    "colours",
    "themes",
    "theming",
    "myriad",
    "customization"
  ],
  "type": "module",
  "main": "./index.umd.cjs",
  "module": "./index.js",
  "types": "./types/index.d.ts",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "require": "./index.umd.cjs",
      "import": "./index.js"
    },
    "./*": "./*"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && copy package.json dist && copy README.md dist",
    "publish": "cd dist && npm publish"
  },
  "devDependencies": {
    "@types/node": "^18.15.3",
    "@types/tinycolor2": "^1.4.3",
    "typescript": "^4.9.3",
    "vite": "^4.1.0",
    "vite-plugin-dts": "^2.1.0"
  },
  "dependencies": {
    "@types/apca-w3": "^0.1.0",
    "apca-w3": "^0.1.9",
    "colorparsley": "^0.1.8",
    "tinycolor2": "^1.4.2"
  }
}
